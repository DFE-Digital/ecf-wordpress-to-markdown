{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-useless-escape": 0,
        "no-param-reassign": 0,
        "no-console": 0,
        "import/prefer-default-export": 0,
        "consistent-return": 0
    }
}
